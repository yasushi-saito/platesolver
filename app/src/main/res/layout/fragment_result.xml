<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fragment_result"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_left"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_end="16dp" />

    <TextView
        android:id="@+id/text_result_imagename"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="imagename"
        app:layout_constraintBaseline_toBaselineOf="@+id/label_image"
        app:layout_constraintStart_toStartOf="@+id/guideline2" />

    <TextView
        android:id="@+id/label_image"
        style="@style/HeaderText"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Image"
        app:layout_constraintStart_toStartOf="@+id/guideline_left0"
        app:layout_constraintTop_toBottomOf="@+id/view_annotatedimage"
        tools:text="Image" />

    <TextView
        android:id="@+id/label_imagedimension"
        style="@style/HeaderText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Dimension"
        app:layout_constraintStart_toStartOf="@+id/guideline_left1"
        app:layout_constraintTop_toBottomOf="@+id/label_image" />

    <TextView
        android:id="@+id/label_lastupdate"
        style="@style/HeaderText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/guideline_left1"
        app:layout_constraintTop_toBottomOf="@+id/label_imagedimension" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_left1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="51dp" />

    <TextView
        android:id="@+id/label_imagedimension2"
        style="@style/HeaderText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Dimension"
        app:layout_constraintStart_toStartOf="@+id/guideline_left1"
        app:layout_constraintTop_toBottomOf="@+id/label_imagedimension" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_left0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="16dp" />

    <TextView
        android:id="@+id/label_fov"
        style="@style/HeaderText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Field of view (deg)"
        app:layout_constraintStart_toStartOf="@+id/guideline_left0"
        app:layout_constraintTop_toBottomOf="@+id/label_lastupdate" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="181dp" />

    <TextView
        android:id="@+id/text_result_imagedimension"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="dimension"
        app:layout_constraintBaseline_toBaselineOf="@+id/label_imagedimension"
        app:layout_constraintStart_toStartOf="@+id/guideline2" />

    <TextView
        android:id="@+id/text_result_imagelastupdate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="lastupdate"
        app:layout_constraintBaseline_toBaselineOf="@+id/label_imagedimension2"
        app:layout_constraintStart_toStartOf="@+id/guideline2" />

    <TextView
        android:id="@+id/text_result_fov_deg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="fov"
        app:layout_constraintBaseline_toBaselineOf="@+id/label_fov"
        app:layout_constraintStart_toStartOf="@+id/guideline2" />

    <TextView
        android:id="@+id/label_show_objects"
        style="@style/HeaderText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Show objects "
        app:layout_constraintStart_toStartOf="@+id/guideline_left0"
        app:layout_constraintTop_toBottomOf="@+id/label_fov" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBaseline_toBaselineOf="@+id/label_show_objects"
        android:text="Fewer"
        app:layout_constraintStart_toStartOf="@+id/guideline2" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="More"
        app:layout_constraintBaseline_toBaselineOf="@+id/label_show_objects"
        app:layout_constraintEnd_toStartOf="@+id/guideline_left" />

    <SeekBar
        android:id="@+id/seekbar_result_matchedstars"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:progress="100"
        app:layout_constraintBottom_toBottomOf="@+id/textView6"
        app:layout_constraintEnd_toStartOf="@+id/textView7"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        tools:layout_editor_absoluteY="523dp" />

    <com.yasushisaito.platesolver.AnnotatedImageView
        android:id="@+id/view_annotatedimage"
        android:layout_width="0dp"
        android:layout_height="580dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>
