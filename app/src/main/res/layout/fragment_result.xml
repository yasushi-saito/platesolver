<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fragment_result"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_left"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_end="16dp" />

    <TextView
        android:id="@+id/text_result_imagename"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="imagename"
        app:layout_constraintBaseline_toBaselineOf="@+id/label_image"
        app:layout_constraintStart_toStartOf="@+id/guideline2" />

    <TextView
        style="@style/HeaderText"
        android:id="@+id/label_image"
        android:layout_width="0dp"
        android:layout_height="19dp"
        android:layout_marginTop="24dp"
        android:text="Image"
        app:layout_constraintStart_toStartOf="@+id/guideline_left0"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="Image" />

    <TextView
        style="@style/HeaderText"
        android:id="@+id/label_imagedimension"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Dimension"
        app:layout_constraintStart_toStartOf="@+id/guideline_left1"
        app:layout_constraintTop_toBottomOf="@+id/label_image" />

    <TextView
        style="@style/HeaderText"
        android:id="@+id/label_lastupdate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/guideline_left1"
        app:layout_constraintTop_toBottomOf="@+id/label_imagedimension" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_left1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="51dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_left0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="16dp" />

    <TextView
        style="@style/HeaderText"
        android:id="@+id/label_fov"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Field of view (deg)"
        app:layout_constraintStart_toStartOf="@+id/guideline_left0"
        app:layout_constraintTop_toBottomOf="@+id/label_lastupdate" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="234dp" />

    <TextView
        android:id="@+id/text_result_imagedimension"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="dimension"
        app:layout_constraintBaseline_toBaselineOf="@+id/label_imagedimension"
        app:layout_constraintStart_toStartOf="@+id/guideline2" />

    <TextView
        android:id="@+id/text_result_imagelastupdate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="lastupdate"
        app:layout_constraintBaseline_toBaselineOf="@+id/label_lastupdate"
        app:layout_constraintStart_toStartOf="@+id/guideline2" />

    <TextView
        android:id="@+id/text_result_fov_deg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="fov"
        app:layout_constraintBaseline_toBaselineOf="@+id/label_fov"
        app:layout_constraintStart_toStartOf="@+id/guideline2" />

    <TextView
        style="@style/HeaderText"
        android:id="@+id/label_corners2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Show matched objects"
        app:layout_constraintStart_toStartOf="@+id/guideline_left0"
        app:layout_constraintTop_toBottomOf="@+id/label_fov" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="More"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView6"
        app:layout_constraintEnd_toStartOf="@+id/guideline_left" />

    <SeekBar
        android:id="@+id/seekbar_result_matchedstars"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:progress="100"
        app:layout_constraintEnd_toStartOf="@+id/textView7"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/label_corners2" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:text="Fewer"
        app:layout_constraintBottom_toBottomOf="@+id/seekbar_result_matchedstars"
        app:layout_constraintStart_toStartOf="@+id/guideline_left0" />

    <com.yasushisaito.platesolver.AnnotatedImageView
        android:id="@+id/view_annotatedimage"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/seekbar_result_matchedstars"
        app:layout_constraintVertical_bias="1.0" />

</androidx.constraintlayout.widget.ConstraintLayout>
