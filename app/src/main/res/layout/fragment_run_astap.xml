<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fragment_setup"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/text_setup_searchstart_ra_dec"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:maxLines="2"
        android:minLines="2"
        android:text=""
        app:layout_constraintBaseline_toBaselineOf="@+id/autocomplete_astap_searchstart"
        app:layout_constraintStart_toEndOf="@+id/autocomplete_astap_searchstart" />

    <Button
        android:id="@+id/astap_pick_file"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="64dp"
        android:text="Pick file"
        style="@style/Theme.PlateSolver"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="16dp" />

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/barrier2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierDirection="left"
        app:constraint_referenced_ids="fragment_setup" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_end="283dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="deg)"
        app:layout_constraintBaseline_toBaselineOf="@+id/text_astap_fov_deg"
        app:layout_constraintStart_toEndOf="@+id/text_astap_fov_deg" />

    <EditText
        android:id="@+id/text_astap_fov_lens"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:digits="0123456789."
        android:ems="10"
        android:imeOptions="actionNext"
        android:inputType="number"
        app:layout_constraintBaseline_toBaselineOf="@+id/label_setup_fov"
        app:layout_constraintStart_toStartOf="@+id/guideline4" />

    <TextView
        style="@style/HeaderText"
        android:id="@+id/label_setup_fov"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Field of view"
        app:layout_constraintStart_toStartOf="@+id/guideline3"
        app:layout_constraintTop_toBottomOf="@+id/astap_pick_file" />

    <EditText
        android:id="@+id/text_astap_fov_deg"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:digits="0123456789."
        android:ems="10"
        android:imeOptions="actionNext"
        android:inputType="text|number"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView3"
        app:layout_constraintStart_toEndOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="mm FX lens ("
        app:layout_constraintBaseline_toBaselineOf="@+id/text_astap_fov_lens"
        app:layout_constraintStart_toEndOf="@+id/text_astap_fov_lens" />

    <Button
        android:id="@+id/setup_run"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="48dp"
        android:text="Run"
        style="@style/Theme.PlateSolver"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/text_setup_imagename"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:text="imagename"
        app:layout_constraintBaseline_toBaselineOf="@+id/astap_pick_file"
        app:layout_constraintStart_toEndOf="@+id/astap_pick_file" />

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/barrier3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierDirection="left"
        app:constraint_referenced_ids="fragment_setup" />

    <AutoCompleteTextView
        android:id="@+id/autocomplete_astap_searchstart"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:imeOptions="actionNext"
        android:inputType="text"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView8"
        app:layout_constraintStart_toStartOf="@+id/guideline4" />

    <TextView
        style="@style/HeaderText"
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Search start"
        app:layout_constraintStart_toStartOf="@+id/guideline3"
        app:layout_constraintTop_toBottomOf="@+id/text_astap_fov_lens" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="313dp" />

</androidx.constraintlayout.widget.ConstraintLayout>
